{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "singularName": "book",
    "pluralName": "books",
    "displayName": "book",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "published": {
      "type": "datetime"
    },
    "author": {
      "type": "string"
    },
    "pages": {
      "type": "integer"
    },
    "publisher": {
      "type": "string"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review"
    },
    "genres": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::genre.genre"
    }
  }
}
